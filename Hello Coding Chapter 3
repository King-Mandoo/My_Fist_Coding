using System;

namespace ConsoleWrite
{
    class Program
    {
        static void Main(string[] args)
        {
            int number1 = 286;
            double number2 = 3.141592;
            string sentence = "문자열도 있음";

            Console.WriteLine(number1);
            Console.WriteLine(number2);
            Console.WriteLine(sentence);
        }
    }
}
